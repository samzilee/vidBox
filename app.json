{
  "expo": {
    "name": "vidBox",
    "slug": "vidBox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/cover.png",
    "scheme": "vidbox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.vidBox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/default.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.vidBox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/default.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/default.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "27d2d24e-6487-4157-af94-2e8ac8a7714c"
      }
    }
  }
}
